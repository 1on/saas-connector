extensions:
	- php
	
source:
	- src
	
exclude:
	- tests/
	- vendor/
	- bin/
	- docs/
	
charset:
	- auto
	- UTF-8
	- Windows-1251
	
title: SaaS Connector - API client collection
templateTheme: bootstrap
groups: auto
autocomplete:
	- classes
	- constants
	- methods
	- classconstants
	
accessLevels:
	- public
	
internal: true
php: false
tree: true
deprecated: true
todo: true
destination: ../saas-connector.pages/
download: false
