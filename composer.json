{
    "name": "gwinn/saas-connector",
    "license": "MIT",
    "description": "Set of SaaS clients",
    "type": "library",
    "keywords": ["api", "saas", "rest"],
    "homepage": "http://github.com/gwinn/saas-connector",
    "authors": [
        {
            "name": "Alex Lushpai",
            "email": "lushpai@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Anna Mazepina",
            "email": "putintseva@retailcrm.ru",
            "role": "Developer"
        },
        {
            "name": "Azgalot",
            "email": "artahanov@retailcrm.ru",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "lushpai@gmail.com"
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "league/oauth2-client": "1.4.2",
        "doctrine/annotations": "^1.6",
        "symfony/validator": "^4.3",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer": "^1.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "er1z/fakemock": "^0.3.1",
        "phpstan/phpstan": "^0.11.8",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "SaaS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SaaS\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "process-timeout": 600
    }
}
